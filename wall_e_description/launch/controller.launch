<launch>

<rosparam file="$(find wall_e_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="wall_e" args="left_drive_position_controller right_drive_position_controller pan_joint_position_controller pan_left_arm_position_controller pan_right_arm_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/wall_e/joint_states"/>
</node>

</launch>